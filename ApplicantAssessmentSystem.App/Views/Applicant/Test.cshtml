@section Scripts {
    <script>
        $(document).ready(function () {

            function timer() {
                document.getElementById("testTimer").style.display = "block"
                document.getElementById("minute").innerText = "1";
                document.getElementById("second").innerText = "59";
                const myTimeout = setInterval(function () {
                    let minute = Number(document.getElementById("minute").innerText);
                    let second = Number(document.getElementById("second").innerText);

                    second = second - 1;
                    let secondStringValue = second.toString();
                    if (secondStringValue.length === 1)
                        secondStringValue = "0" + secondStringValue;
                    document.getElementById("second").innerText = secondStringValue;


                    if (second === 0) {
                        //check current minute
                        let currentMinute = Number(document.getElementById("minute").innerText);

                        if (currentMinute === 0) {
                            clearTimeout(myTimeout);
                            //submit test for the ongoin subject
                            let currentSubject = $('#currentSubject').val();
                            debugger;
                            if (currentSubject === "General Knowledge") {
                                submitGeneralKnowledge();
                            }
                            else if (currentSubject === "Mathematics") {
                                submitMathematics();
                            }
                            else if (currentSubject === "Computer Technology") {
                                submitComputerTechnology();
                            }
                        }

                        if (currentMinute > 0) {
                            document.getElementById("second").innerText = "59";
                        }

                        minute = minute - 1;

                        let minuteStringValue = second.toString();
                        if (minuteStringValue.length === 1)
                            minuteStringValue = "0" + minuteStringValue;
                        document.getElementById("minute").innerText = minuteStringValue;

                    }

                }, 1000);
            }

            $('#startGeneralKnowledgeButton').click(function (e) {
                debugger;
                e.preventDefault();
                $('#currentSubject').val("General Knowledge");
                getAnswerDetailByApplicantId();
                //timer(); // timer call after checking if candidate has doe the test or not
            })

            $('#startMathematicsButton').click(function (e) {
                debugger;
                e.preventDefault();
                $('#currentSubject').val("Mathematics");
