            function prepareAnswerObject(applicantAnswerSummaryId, sessionId, questionNumber, score, applicantId, subject, selectedAnswer, isLastSubject) {
                debugger;
                var self = this;

                self.ApplicantAnswerSummaryId = applicantAnswerSummaryId;
                self.SessionId = sessionId;
                self.QuestionNumber = Number(questionNumber);
                self.Score = score;
                self.ApplicantId = Number(applicantId);
                self.Subject = subject;
                self.SelectedAnswer = selectedAnswer;
                self.IsLastSubject = isLastSubject;
            }

